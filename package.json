{
  "name": "@distributeaid/flexport-api-sandbox",
  "version": "0.0.0-development",
  "description": "Provides a sandbox host of the Flexport API v2.",
  "scripts": {
    "start": "node dist/start.js"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/distributeaid/flexport-api-sandbox.git"
  },
  "keywords": [
    "Distribute Aid",
    "Flexport"
  ],
  "author": "Distribute Aid | https://distributeaid.org/",
  "license": "AGPL-3.0-only",
  "bugs": {
    "url": "https://github.com/distributeaid/flexport-api-sandbox/issues"
  },
  "homepage": "https://github.com/distributeaid/flexport-api-sandbox#readme",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --ext .js"
    ],
    "*.{md,json,yaml,yml,js}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@bifravst/code-style": "^7.1.4",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-angular": "^8.3.4",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.13.2",
    "@types/node-fetch": "^2.5.7",
    "@types/whatwg-fetch": "0.0.33",
    "husky": "^4.2.5",
    "jest": "^25.4.0",
    "node-fetch": "^2.6.0",
    "ts-jest": "^25.4.0",
    "typescript": "^3.8.3",
    "whatwg-fetch": "^3.0.0"
  },
  "release": {
    "branches": [
      "saga"
    ],
    "remoteTags": true
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "sandbox",
    "dist",
    "package-lock.json",
    "LICENSE",
    "README.md"
  ],
  "jest": {
    "testURL": "http://localhost",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": ".+\\.spec\\.ts$",
    "coverageReporters": [
      "html",
      "lcov"
    ],
    "globals": {
      "ts-jest": {
        "diagnostics": true
      }
    }
  },
  "dependencies": {}
}
